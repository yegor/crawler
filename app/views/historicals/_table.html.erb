<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Date</th>
      <th>Hour</th>
      <th>Rank</th>
    </tr>
  </thead>
  <tbody>
    <% @table_rankings.keys.sort.reverse.each do |date| %>
    <tr>
      <td><%= date.strftime("%d %h") %></td>
      <td><%= date.hour %></td>
      <td><%= @table_rankings[date].rank %></td>
    </tr>
    <% end %>
  </tbody>
</table>